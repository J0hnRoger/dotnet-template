{
  "$schema": "http://json.schemastore.org/template",
  "author": "John Roger",
  "classifications": [
    "Web",
    "FrontEnd",
    "React",
    "Redux",
    "Clean Architecture"
  ],
  "name": "React Solution Templates",
  "description": "A pragmatic React / Redux solution with Clean Architecture and functional programming in mind",
  "identity": "DotNet.Architecture.Solution.React",
  "groupIdentity": "DotNet.Architecture.FrontEnd",
  "shortName": "jr-react",
  "tags": {
    "language": "TS",
    "type": "project"
  },
  "sourceName": "DotnetTemplate",
  "preferNameDirectory": true,
  "symbols": {
    "jrRepositoryUrl": {
      "type": "generated",
      "generator": "constant",
      "replaces": "jrRepositoryUrl",
      "parameters": {
        "value": "https://github.com/J0hnRoger/dotnet-template"
      }
    },
    "jrrPackageVersion": {
      "type": "generated",
      "generator": "constant",
      "replaces": "jrPackageVersion",
      "parameters": {
        "value": "1.0.0"
      }
    }
  },
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        ".template.config/**/*",
        "templates/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "*.nuspec",
        ".vs/**",
        ".git/**",
        ".idea/**",
        ".scripts/publish.ps1"
      ],
      "rename": {
        "README-template.md": "README.md"
      },
      "modifiers": [
        {
          "exclude": []
        }
      ]
    }
  ]
}
