{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared",
    "IdentityConnection": "DataSource=identity.db;Cache=Shared"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "domain.com",
    "TenantId": "<tenantId>",
    "ClientId": "<clientId>",
    "ClientSecret": "<clientSecret>",
    "CallbackPath": "/signin-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ApplicationInsights": {
        "LogLevel": {
          "Default": "Trace",
          "Microsoft": "Error"
        }
      }
    }
  }
}
